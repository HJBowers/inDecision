  "name": "inDecision",
  "version": "1.0.0",
  "description": "An app that calls a Lyft to take you to a randomly selected restaurant or bar.",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
