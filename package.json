    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "node-fetch": "^2.0.0",
    "node-lyft": "^1.0.3",
    "node-persist": "^2.1.0",
    "passport": "^0.4.0",
